COLUMN TABLE "contaOre.db.data::Ore" (
     "Progetti.nome" NVARCHAR(60) COMMENT 'Progetto',
     "Tasks.nomeTask" NVARCHAR(60) COMMENT 'Task',
     "user" NVARCHAR(60) COMMENT 'Utente',
     "giorno" NVARCHAR(10) COMMENT 'Giorno',
     "numeroOre" INTEGER COMMENT 'Numero Ore',
     PRIMARY KEY ("Progetti.nome", "Tasks.nomeTask", "user", "giorno")
)