COLUMN TABLE "Ore" (
     "Tasks.nome" NVARCHAR(60) COMMENT 'Progetto',
     "Tasks.nomeTask" NVARCHAR(60) COMMENT 'Task',
     "user" NVARCHAR(60) COMMENT 'Utente',
     "giorno" DATA COMMENT 'Giorno',
     "numeroOre" INTEGER COMMENT 'Numero Ore'
     PRIMARY KEY ("Tasks.nome", "Tasks.nomeTask", "user", "giorno")
)